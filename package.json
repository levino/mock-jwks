{
  "name": "mock-jwks",
  "description": "A tool to mock a JWKS for development of microservices who consume jwts signed with RSA",
  "version": "3.3.1",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
